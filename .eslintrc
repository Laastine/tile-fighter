{
  "env": {
    "browser": true,
    "es6": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "modules": true
  },
  "extends": "eslint:recommended",
  "plugins": [],
  "rules": {
    "quotes": [2, "single"],
    "no-use-before-define": [2, "nofunc"],
    "no-console": [2],
    "no-underscore-dangle": [2],
    "no-unused-vars": [2, {"args": "none"}],
    "no-unused-expressions": [0],
    "camelcase": [2],
    "new-cap": [2],
    "space-infix-ops": [2],
    "no-loop-func": [0],
    "semi": [2, "never"],
    "quotes": [2, "single"],
    "no-alert": [2],
    "no-extra-bind": [2],
    "no-sequences": [2]
  }
}